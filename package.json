{
  "name": "@friendsoftheweb/utils",
  "repository": {
    "url": "git+https://github.com/friendsoftheweb/utils.git"
  },
  "version": "0.10.0",
  "type": "module",
  "license": "MIT",
  "packageManager": "yarn@4.12.0",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./node": {
      "import": {
        "types": "./dist/node.d.mts",
        "default": "./dist/node.mjs"
      },
      "require": {
        "types": "./dist/node.d.cts",
        "default": "./dist/node.cjs"
      }
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "types:check": "tsc --noEmit",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint .",
    "build": "tsdown",
    "prepack": "tsdown",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@types/jest": "^30.0.0",
    "@types/lodash-es": "^4",
    "@types/node": "^24.10.1",
    "babel-jest": "^30.2.0",
    "eslint": "^9.39.1",
    "eslint-plugin-perfectionist": "^4.15.1",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.6.2",
    "tsdown": "^0.20.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0"
  },
  "dependencies": {
    "@date-fns/tz": "^1.0.0",
    "date-fns": "^4.0.0",
    "lodash-es": "^4.17.21"
  }
}
